<% include customerTemplate/afterLoginHeader %>
	
<p>Please choose a branch for <%= data[0].name%>:</p>
<form method="post" action="makeReservation">
	Num of Pax: <input type="number" name="num"></input>
	<br>
	<tbody>
		<% for(var i=0; i<data.length; i++) { %>
		<tr>
			<td><input type="radio" name="branch" value="<%= data[i].id%>"><%= data[i].address%></input></td>
		</tr>
		<% } %>
	</tbody>
	<br>
	<button type="submit">Next</button>
</form>
	
<% include customerTemplate/footer %>
